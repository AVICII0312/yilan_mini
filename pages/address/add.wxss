@import "../../components/templates/divButton.wxss";

.container{
    padding: 16rpx 30rpx 0;
    margin-bottom: 20rpx;
    background: #fff;
    border-radius: 10px;
}
.section{
    display: flex;
    align-items: center;
    padding: 34rpx 0;
    border-bottom: 2rpx solid #dedede;
}
.section:last-child{
    border: none;
}
.section .name{
    margin-right: 36rpx;
    width: 112rpx;
    font-size: 26rpx;
    font-weight: 400;
    color: #333;
}
.section .input{
    flex: 1;
    font-size: 24rpx;
    font-weight: 400;
    color: #333;
}
.section .placeholder{
    color: #ccc;
}
.button {
    position: fixed;
    left: 30rpx;
    bottom: 20rpx;
    color: #fff;
  }
  